package com.vti.exercise_4;

import java.util.Scanner;

public class CanBo {
// Họ tên, tuổi, giới tính(name, nữ,khác), địa chỉ.
	protected String name;
	protected int tuoi;
	protected Enum_canbo gioiTinh;
	protected String address;
	
	public CanBo() {
		Scanner canbo1 = new Scanner(System.in);
		System.out.println("moi nhap thong tin ten: ");
		this.name = canbo1.nextLine();
		System.out.println("moi nhap thong tin tuoi: ");
		this.tuoi = canbo1.nextInt();
		System.out.println("moi nhap thong tin gioi tinh: ");
		this.gioiTinh = Enum_canbo.nam;
		System.out.println("moi nhap thong tin dia chi: ");
		this.address = canbo1.nextLine();
	}
	
	public CanBo (String name,  int tuoi, Enum_canbo gioitinh, String address) {
		this.name = name;
		this.tuoi = tuoi;
		this.gioiTinh = gioitinh;
		this.address = address;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getTuoi() {
		return tuoi;
	}
	public void setTuoi(int tuoi) {
		this.tuoi = tuoi;
	}
	public Enum_canbo getGioiTinh() {
		return gioiTinh;
	}
	public void setgioiTinh(Enum_canbo gioiTinh) {
		this.gioiTinh = gioiTinh;
	}
	public String getaddress() {
		return address;
	}
	public void setaddress(String address) {
		this.address = address;
	}
	
	
	
	
	
}
