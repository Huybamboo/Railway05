package Backend;

import java.util.ArrayList;
import java.util.List;
import Entity.Student;

public class Exercise_1 {
	public void Question_1() {
		List<Student> student = new ArrayList<>();
		student.add(new Student(1,  "Nguyễn Văn A"));
		student.add(new Student(2 , "Nguyễn Văn A"));
		student.add(new Student(3,  "Nguyễn Văn A"));
		student.add(new Student(4,  "Nguyễn Văn B"));
		student.add(new Student(5,  "Nguyễn Văn C"));
		student.add(new Student(6,  "Nguyễn Văn D"));
		student.add(new Student(7,  "Nguyễn Văn F"));
		student.add(new Student(8,  "Nguyễn Văn E"));
		student.add(new Student(9,  "Nguyễn Văn X"));
		student.add(new Student(10, "Nguyễn Văn Y"));
		
		System.out.println("Câu a: ");
		System.out.println("Số lượng học sinh hiện có là: " + Student.getCounter());
		System.out.println("Câu b: ");
		System.out.println("thông tin học sinh thứ 4 là:\n " + student.get(3));
		
	}

}
