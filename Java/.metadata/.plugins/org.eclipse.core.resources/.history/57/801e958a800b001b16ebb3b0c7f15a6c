package backend;

import java.util.ArrayList;
import java.util.List;

import entity.Student;

public class Exercise1 {
	public static void Question1_1() {
		List<Student> students = new ArrayList<>();
		students.add(new Student(1 , "Nguyễn Văn A"));
		students.add(new Student(2 , "Nguyễn Văn A"));
		students.add(new Student(3 , "Nguyễn Văn A"));

		Student.setcollege("Đại học bách khoa");
		for (Student student : students) {
			System.out.println(student);
		}
}
	
	public static void Question1_2() {
		Student.setcollege("Đại học công nghệ");
	}
}