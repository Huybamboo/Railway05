package Backend.Exercise_3;

public class CPU {
	int price;
	
	public class Processor {
		int coreAmount;
		String menufacturer;
	
		
	public void getCache(float a) {
		System.out.println("Cache: " + a);
	}
	}
	
	public class Ram {
		int memory;
		Processor menufacturer;
		
	public void getClockSpeed(float b) {
		System.out.println("Clock Speed: " + b);
		
	}
		
	}
}
