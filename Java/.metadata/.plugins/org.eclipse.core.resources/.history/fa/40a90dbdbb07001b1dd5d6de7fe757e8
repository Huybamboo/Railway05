package entity;

import java.util.Scanner;

public class TuyenSinh extends Quan_ly_hs_thi_DH implements ITuyenSinh{

	public TuyenSinh(int SBD, String name, String address, Muc_uu_tien uuTien){
		super(SBD, name, address, uuTien);
	
	}

	@Override
	public void addHS() {
		
		Scanner scanner = new Scanner(System.in);
		System.out.println("SBD cua hssv can them");
		int SBDx  = scanner.nextInt();
		System.out.println("ten cua hssv can them");
		String namex = scanner.nextLine();
		System.out.println("dia chi cua hssv can them");
		String addressx = scanner.nextLine();
		System.out.println("muc uu tien cua hssv can them(tu Muc_1 - Muc_3)");
		scanner.nextLine();
		String uuTienx = scanner.nextLine();
		this.uuTien = Muc_uu_tien.valueOf(uuTienx);
		
		System.out.println("thong tin sv vua nhap la: " + SBDx + " " + namex + " " + addressx + " " + uuTienx);
		TuyenSinh[] addx = {SBDx, namex, addressx, uuTienx};
		System.out.println(addx);
		
	}

	@Override
	public void show_info() {
		
		
	}

	@Override
	public void search() {
		
		
	}

	@Override
	public void exit() {
		// TODO Auto-generated method stub
		
	}

}
