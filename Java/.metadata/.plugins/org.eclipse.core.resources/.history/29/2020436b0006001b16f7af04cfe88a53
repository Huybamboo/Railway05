package com.vti.exercise_4;

public class CongNhan extends CanBo {
	private int bac;
	
	public CongNhan() {
		super();
		}
	
	public CongNhan (int bac, String name, int tuoi, Enum_canbo gioiTinh, String address) {
		super ( name,  tuoi,  gioiTinh,  address);
		this.bac = bac;
		}
	public int getbac() {
		return bac;
	}
	public void setbac(int bac) {
		this.bac = bac;
	}
}