package frontend;

import java.io.File;

public class ProgramFile {
	public static void main(String[] args) {
		File file = new File ("C:\\Users\\pc\\Desktop\\Test.txt");
		if (file.exists()) {
			System.out.println("file exists");
		} else {
			System.out.println("file not exists");
		}
			
		

	}

}
